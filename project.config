[project]
	description = Welcome repo for ReviewNinja to help you get started. Feel free to delete when you're done, we won't take it personally.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/fordadamj
[access "refs/for/refs/*"]
	read = group user/fordadamj
	push = group user/fordadamj
	label-Code-Review = -2..+2 group user/fordadamj
	label-Verified = -1..+1 group user/fordadamj
	rebase = group user/fordadamj
	removeReviewer = group user/fordadamj
	submit = group user/fordadamj
[access "refs/heads/*"]
	read = group user/fordadamj
	create = group user/fordadamj
	push = +force group user/fordadamj
	pushMerge = group user/fordadamj
[access "refs/tags/*"]
	push = +force group user/fordadamj
	pushTag = group user/fordadamj
	pushSignedTag = group user/fordadamj
